{
  "name": "smartthings-pro",
  "private": true,
  "version": "2.0.0",
  "description": "SmartThings Pro",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "a2tec": "vite --mode a2tec --port 8080",
    "bart": "vite --mode bart --port 8080",
    "sec": "vite --mode sec --port 8080",
    "green": "vite --mode green --port 8080",
    "srph": "vite --mode srph --port 8080",
    "build": "vite build",
    "build-a2tec": "npx --max_old_space_size=4096 vite build --mode a2tec",
    "build-bart": "npx --max_old_space_size=4096 vite build --mode bart",
    "build-local": "npx --max_old_space_size=4096 vite build --mode local",
    "build-dev": "npx --max_old_space_size=4096 vite build --mode dev",
    "build-neudev": "npx --max_old_space_size=4096 vite build --mode neudev",
    "build-stg": "npx --max_old_space_size=4096 vite build --mode stg",
    "build-prod": "npx --max_old_space_size=4096 vite build --mode prod",
    "build-srpol": "npx --max_old_space_size=4096 vite build --mode srpol",
    "build-srph": "npx --max_old_space_size=4096 vite build --mode srph",
    "build-sec": "npx --max_old_space_size=4096 vite build --mode sec",
    "build-green": "npx --max_old_space_size=4096 vite build --mode green",
    "build-exhome": "npx --max_old_space_size=4096 vite build --mode exhome",
    "build-exhome-external": "npx --max_old_space_size=4096 vite build --mode exhome-external",
    "start:local": "cross-env NODE_ENV=local nodemon --exec babel-node backend/server.js",
    "start:dev": "cross-env NODE_ENV=dev nodemon --exec babel-node backend/server.js",
    "start": "cross-env NODE_ENV=prod babel-node backend/server.js",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@vueuse/core": "^11.1.0",
    "axios": "^1.7.7",
    "builder-pattern": "^2.2.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "echarts": "^5.5.1",
    "echarts-stat": "^1.2.0",
    "file-saver": "^2.0.5",
    "google-libphonenumber": "^3.2.38",
    "html2canvas": "^1.4.1",
    "jquery": "^3.7.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^2.5.1",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^4.1.2",
    "qs": "^6.13.0",
    "quill": "^2.0.2",
    "sanitize-html": "^2.13.0",
    "sockjs-client": "^1.6.1",
    "v3-click-outside": "^1.1.0",
    "vue": "^3.4.21",
    "vue-echarts": "^7.0.3",
    "vue-i18n": "^9.13.1",
    "vue-moment": "^4.1.0",
    "vue-router": "^4.3.0",
    "vue3-google-map": "^0.21.0",
    "vue3-shortkey": "^4.0.0",
    "webstomp-client": "^1.2.6",
    "whatwg-fetch": "^3.6.20",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.0.0-rc.17",
    "@vue/vue3-jest": "^29.2.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.26.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.3.2",
    "sass": "^1.77.6",
    "vite": "^5.2.0",
    "vite-plugin-node-polyfills": "^0.22.0"
  }
}
